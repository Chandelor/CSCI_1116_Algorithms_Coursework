import java.util.Scanner;

public class Exercise_22_1 {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		
		System.out.print("Enter a String: ");
		String userString = input.next();
		
		String currentString = "";
		String maxString = "";
		for (int i = 0; i < userString.length(); i++) {
			
			// Runs if string does not contain specified character.
			if (currentString.indexOf(userString.charAt(i)) < 0) {
				currentString += userString.charAt(i);
				System.out.println(currentString);
			}
			
			// Stores largest String.
			else if (currentString.length() > maxString.length()) maxString = currentString;
			// Runs if string contains specified character.
			else currentString = "" + userString.charAt(i);
			
		}
		
		System.out.println(maxString);
	}

}
